def Plus(num1,num2):
    z = num1+num2
    return z
def Min(num1,num2):
    z = num1 - num2
    return z
def Mul(num1,num2):
    z = num1 * num2
    return z
def Div(num1,num2):
    z = num1 / num2
    return z

